{#
   Base template
#}

{% if form is defined and not horizontal is defined %}
    {% form_theme form 'bootstrap_3_layout.html.twig' %}
{% endif %}

{% if horizontal is defined %}
    {% form_theme form 'bootstrap_3_horizontal_layout.html.twig' %}
{% endif %}


<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="robots" content="noindex, nofollow, noarchive, nosnippet, noodp, noimageindex, notranslate, nocache" />
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <meta name="generator" content="Trinity" />        
        <title>{% block title %}Trinity{% endblock %}</title>

        {% block stylesheets %}
            <script src="{{ asset('bundles/libu/jquery/dist/jquery.js') }}"></script>   <!-- Debe ir arriba -->
            <script src="{{ asset('bundles/libu/bootstrap/dist/js/bootstrap.min.js') }}" ></script>
            <script src="{{ asset('bundles/libu/js/scripts.js') }}" ></script>
            <link rel="stylesheet" href="{{ asset('bundles/libu/bootstrap/dist/css/bootstrap.min.css') }}"  /> 
            <link rel="stylesheet" href="{{ asset('bundles/libu/bootstrap/dist/css/bootstrap-theme.css') }}"  />  
            <link rel="stylesheet" href="{{ asset('bundles/libu/css/style.css') }}" />            
        {% endblock %}

        {#  <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />    #}
        {% block head_cont %}{% endblock %}
    </head>

    <body id="{% block body_id %}body_id{% endblock %}">

        <div class='container-fluid'>
            {% block header %}
                <header>
                </header>

            {% endblock %}


            {% block navbar %}

            {% endblock %}

            {% block body %}

                {% block formulario %}

                {% endblock %}     
                 
            {% endblock %}


            {% block footer %}
                <footer>    
                </footer>
            {% endblock %}   
        </div>
    </body>
</html>